            steps {
                sh 'echo "Building stage"'
            }
        }
        stage('Test') { 
            steps {
                sh 'echo "Testing stage"'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo "Deployment stage"'
            }
        }
    }
}